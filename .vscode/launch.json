{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
        "name": "Workflow GLSP Example Extension",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
            "${workspaceFolder}/example/workflow/workspace",
            "--extensionDevelopmentPath=${workspaceFolder}/packages/nmf-vscode-integration"
        ],
        "outFiles": [
            "${workspaceRoot}/packages/nmf-vscode-integration/dist/workflow-extension.js",
            "${workspaceRoot}/packages/nmf-vscode-integration/lib/**/*.js",
            "${workspaceRoot}/packages/vscode-integration/lib/**/*.js",
            "${workspaceRoot}/node_modules/@eclipse-glsp/*/lib/**/*.js",
            "${workspaceRoot}/node_modules/vscode-messenger/*/lib/**/*.js",
            "${workspaceRoot}/node_modules/vscode-messenger-common/*/lib/**/*.js"
        ],
        "sourceMaps": true,
        "sourceMapPathOverrides": {
            "webpack://workflow-vscode-example/(.+)": "${workspaceFolder}/packages/nmf-vscode-integration/$1"
        },
        "env": {
            "GLSP_LOG_DIR": "${workspaceFolder}/logs"
        }
        },
        {
        "name": "Workflow GLSP Example Extension (External GLSP Server)",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
            "${workspaceFolder}/example/workflow/workspace",
            "--extensionDevelopmentPath=${workspaceFolder}/packages/nmf-vscode-integration"
        ],
        "outFiles": [
            "${workspaceRoot}/packages/nmf-vscode-integration/dist/workflow-extension.js",
            "${workspaceRoot}/packages/nmf-vscode-integration/lib/**/*.js",
            "${workspaceRoot}/packages/vscode-integration/lib/**/*.js",
            "${workspaceRoot}/node_modules/@eclipse-glsp/*/lib/**/*.js"
        ],
        "sourceMaps": true,
        "sourceMapPathOverrides": {
            "webpack://workflow-vscode-example/(.+)": "${workspaceFolder}/packages/nmf-vscode-integration/$1"
        },
        "env": {
            "GLSP_SERVER_DEBUG": "true",
            "GLSP_SERVER_PORT": "5052"
        }
        },
        {
        "name": "Workflow GLSP Example Extension (WebSocket)",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
            "${workspaceFolder}/example/workflow/workspace",
            "--extensionDevelopmentPath=${workspaceFolder}/packages/nmf-vscode-integration"
        ],
        "outFiles": [
            "${workspaceRoot}/packages/nmf-vscode-integration/dist/workflow-extension.js",
            "${workspaceRoot}/packages/nmf-vscode-integration/lib/**/*.js",
            "${workspaceRoot}/packages/vscode-integration/lib/**/*.js",
            "${workspaceRoot}/node_modules/@eclipse-glsp/*/lib/**/*.js"
        ],
        "sourceMaps": true,
        "sourceMapPathOverrides": {
            "webpack://workflow-vscode-example/(.+)": "${workspaceFolder}/packages/nmf-vscode-integration/$1"
        },
        "env": {
            "GLSP_LOG_DIR": "${workspaceFolder}/logs",
            "GLSP_WEBSOCKET_PATH": "glsp"
        }
        },
        {
        "name": "Workflow GLSP Example Extension (External WebSocket GLSP Server)",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
            "${workspaceFolder}/example/workspace",
            "--extensionDevelopmentPath=${workspaceFolder}/packages/nmf-vscode-integration"
        ],
        "outFiles": [
            "${workspaceRoot}/packages/nmf-vscode-integration/dist/workflow-extension.js",
            "${workspaceRoot}/packages/nmf-vscode-integration/lib/**/*.js",
            "${workspaceRoot}/node_modules/@eclipse-glsp/*/lib/**/*.js"
        ],
        "sourceMaps": true,
        "sourceMapPathOverrides": {
            "webpack://nmf-vscode-integration/(.+)": "${workspaceFolder}/packages/nmf-vscode-integration/$1"
        },
        "env": {
            "GLSP_LOG_DIR": "${workspaceFolder}/logs",
            "GLSP_SERVER_DEBUG": "true",
            "GLSP_SERVER_PORT": "5052",
            "GLSP_WEBSOCKET_PATH": "glsp"
        }
        }
    ]
}